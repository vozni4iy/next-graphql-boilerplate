{
	"parser": "babel-eslint",
	"plugins": [
		"react"
	],
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended"
	],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true,
			"modules": true
		}
	},
	"env": {
		"browser": true,
		"es6": true
	},
	"rules": {
		"no-console": [
			0
		],
		"react/display-name": [0, { "ignoreTranspilerName": 0 }]
	}
}